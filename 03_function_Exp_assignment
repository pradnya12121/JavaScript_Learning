    var str="JavaScript the most popular language";
      var len=str.length;
      console.log("Total character in string  :- "  +  len);

      var ind=str.indexOf('S');
      console.log("Index of character 'S' :-  " + ind);

      var ind1=str.indexOf("lang");
      console.log("Index of string 'lang' :- " + ind1 );

      var char=str.charAt(str.length-1);
      console.log("Last character :- " + char);

      
      var char=str.charAt(str.length-3);
      console.log(" 3rd Last character :- " + char);